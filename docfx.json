{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xref": [
      "no"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appName": "OpenMod Docs",
      "_appTitle": "OpenMod Docs",
      "_appFaviconPath": "favicon.ico",
      "_appLogoPath": "logo.png",
      "_enableSearch": true,
      "_disableAffix": true,
      "_appFooter": "OpenMod .NET Plugin Framework.",
      "_disableSideFilter": true,
      "_gitContribute": {
        "repo": "https://github.com/openmod/openmod"
      },
      "_gitUrlPattern": "github"
    },
    "fileMetadataFiles": [],
    "template": [ "default", "templates/discordfx" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}